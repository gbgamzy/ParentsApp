    "version": "0.2.0",
    "configurations": [
        {   
            
            "type": "node",
            "request": "launch",
            "name": "Server Node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server_node/parents.js",
            "cwd": "${workspaceFolder}/server_node",
            "restart": true,
        },
        {
            "type": "dart",
            "request": "launch",
            "name": "TeenCareWeb",
            "program": "${workspaceFolder}/TeenCareWeb",
            "cwd": "${workspaceFolder}/TeenCareWeb",
        },
        {
            "type": "dart",
            "request": "launch",
            "name": "ParentsApp",
            "program": "${workspaceFolder}/parentsApp/lib/main.dart",
            "cwd": "${workspaceFolder}/parentsApp",
        }
    ]
}